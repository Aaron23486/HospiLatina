@model HospiLatina.Data.Entities.DetalleFactura

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Detalle de Factura</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Subtotal)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Subtotal)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cantidad)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cantidad)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Factura)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Factura.IdFactura)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Procedimiento)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Procedimiento.IdProcedimiento)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.IdDetalle" class="btn btn-outline-primary">Editar</a>
    <a asp-action="Index" class="btn btn-outline-secondary ml-2">Regresar</a>
</div>
