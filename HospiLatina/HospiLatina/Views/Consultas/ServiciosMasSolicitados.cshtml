@model IEnumerable<dynamic>

<div class="card mt-4">
    <div class="card-header">
        <h2>Servicios Ordenados por los Más Solicitados</h2>
    </div>
    <div class="card-body">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Servicio</th>
                    <th>Veces Solicitado</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Servicio</td>
                        <td>@item.VecesSolicitado</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
