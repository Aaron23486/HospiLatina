@model IEnumerable<dynamic>

<div class="card mt-4">
    <div class="card-header">
        <h2>Top 10 de Servicios que Más Ganancias Generaron en el Mes</h2>
    </div>
    <div class="card-body">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Servicio</th>
                    <th>Ganancias</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Servicio</td>
                        <td>@item.Ganancias.ToString("C")</td> <!-- Formateo de ganancias a moneda -->
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
